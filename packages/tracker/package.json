{
  "name": "@loglib/tracker",
  "version": "0.4.0",
  "description": "A simple tracker for loglib",
  "keywords": [
    "loglib",
    "event tracker",
    "web analytics",
    "analytics"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Berekte Engida",
  "license": "MIT",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest watch",
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "browser": "./dist/react/index.js",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "react": [
        "dist/react/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@changesets/changelog-git": "^0.1.14",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.15.13",
    "@types/react": "^18.2.0",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitest/browser": "^0.31.1",
    "happy-dom": "^9.20.3",
    "react": "^18.2.0",
    "sinon": "^15.0.4",
    "terser": "^5.17.3",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.0"
  }
}
